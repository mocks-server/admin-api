{
  "name": "@mocks-server/plugin-admin-api",
  "version": "1.4.3",
  "description": "Plugin for Mocks Server. Provides a REST API for administrating settings, fixtures and behaviors",
  "keywords": [
    "mocks-server-plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/plugin-admin-api",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js src test jest.config.js jest.acceptance.config.js",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:acceptance": "jest --config=jest.acceptance.config.js --runInBand",
    "test:coverage": "npm run test",
    "test:ci": "npm run test && npm run test:acceptance",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "@mocks-server/core": "^1.3.0"
  },
  "dependencies": {
    "@mocks-server/admin-api-paths": "1.0.6",
    "@hapi/boom": "9.1.0",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mocks-server/core": "1.4.6",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.3",
    "fs-extra": "9.0.0",
    "husky": "4.2.5",
    "is-promise": "2.1.0",
    "jest": "25.4.0",
    "lint-staged": "10.1.3",
    "prettier": "2.0.4",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "sinon": "9.0.2",
    "strip-ansi": "6.0.0",
    "tree-kill-sync": "1.0.0"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "index.js": "eslint",
    "jest.config.js": "eslint",
    "jest.acceptance.config.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 13.x"
  }
}
